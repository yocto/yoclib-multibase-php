{
	"name": "yocto/yoclib-multibase",
	"type": "library",
	"description": "This yocLibrary enables your project to encode and decode Multibases in PHP.",
	"keywords":[
		"composer",
		"php",
		"multibase",
		"multiformats",
		"yocto",
		"yoclib"
	],
	"license": "GPL-3.0-or-later",
	"require": {
		"php": "^7.4||^8",
		"ext-mbstring": "*"
    },
	"require-dev": {
		"phpunit/phpunit": "^7||^8||^9"
	},
	"autoload": {
		"psr-4": {
			"YOCLIB\\Multiformats\\Multibase\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"YOCLIB\\Multiformats\\Multibase\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "phpunit --coverage-clover coverage.xml ."
	}
}